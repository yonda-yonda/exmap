{
  "name": "exmap",
  "private": true,
  "description": "Experimental Site of React + Openlayers.",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format:prettier": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:eslint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "format": "yarn format:eslint && yarn format:prettier",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "yarn build --prefix-paths && gh-pages -d public",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.6",
    "@mui/system": "^5.2.6",
    "gatsby": "^4.4.0",
    "geo4326": "^1.3.2",
    "geotiff": "^1.0.9",
    "ol": "^6.10.0",
    "proj4": "^2.7.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.2.2",
    "react-hook-form": "^7.22.5",
    "react-scroll": "^1.8.4"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.17",
    "@types/ol": "^6.5.3",
    "@types/proj4": "^2.5.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby-plugin-material-ui": "^4.1.0",
    "gatsby-plugin-react-helmet-async": "^1.2.0",
    "gh-pages": "^3.2.3",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/src/**/*.test.ts"
    ]
  },
  "homepage": "https://yonda-yonda.github.io/exmap"
}
