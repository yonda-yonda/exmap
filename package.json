{
  "name": "exmap",
  "description": "Experimental Site of React + Openlayers.",
  "private": true,
  "homepage": "https://yonda-yonda.github.io/exmap",
  "scripts": {
    "start": "PORT=3000 craco start",
    "build": "craco build",
    "eject": "react-scripts eject",
    "lint:es": "eslint 'src/**/*.{js,tsx,ts,tsx}'",
    "lint:css": "stylelint 'src/**/*.{js,tsx,ts,tsx}'",
    "lint": "yarn lint:es; yarn lint:css;",
    "deploy": "yarn build; gh-pages -d build",
    "test": "jest"
  },
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/system": "^5.14.18",
    "@types/node": "^16.18.39",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.10",
    "eslint": "^8.27.0",
    "geo4326": "1.7.0-alpha.2",
    "geotiff": "^2.1.0",
    "ol": "^8.2.0",
    "opencv-ts": "^1.3.6",
    "path-browserify": "^1.0.1",
    "proj4": "^2.9.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.23.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.1",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.9.0",
    "satellite.js": "^5.0.0",
    "styled-components": "^5.3.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/jest": "^29.5.10",
    "@types/path-browserify": "^1.0.2",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-scroll": "^1.8.10",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "gh-pages": "^6.1.0",
    "jest": "^29.7.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^29.1.1",
    "webpack": "^5.88.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "testMatch": [
      "**/src/**/*.test.ts"
    ]
  }
}
