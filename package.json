{
  "name": "exmap",
  "private": true,
  "description": "Experimental Site of React + Openlayers.",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "yarn build --prefix-paths && gh-pages -d public",
    "test": "jest"
  },
  "dependencies": {
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.6",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@mui/styled-engine-sc": "^5.1.0",
    "gatsby": "^4.4.0",
    "geo4326": "^1.3.0",
    "ol": "^6.10.0",
    "proj4": "^2.7.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.2.2",
    "react-hook-form": "^7.22.5",
    "react-scroll": "^1.8.4",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.8",
    "@types/node": "^16.11.17",
    "@types/ol": "^6.5.3",
    "@types/proj4": "^2.5.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-scroll": "^1.8.3",
    "@types/styled-components": "^5.1.19",
    "gatsby-plugin-react-helmet-async": "^1.2.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
    "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/src/**/*.test.ts"
    ]
  },
  "homepage": "https://yonda-yonda.github.io/exmap"
}
